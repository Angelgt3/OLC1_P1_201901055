##Este es un ejemplo de un archivo de reportería FCA

GenerarReporteEstadistico{

    definirglobales{
        string reporteResumen = "Reporte de Archivo file_1.js de los proyectos";

        ## variables para Reportes de Barras

        Double pr1 = ${PuntajeEspecifico,"Archivo1.js","variable","triangle_draw"};
        Double pr2 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_triangle_i"};
        Double pr3 = ${PuntajeEspecifico,"Archivo1.js","variable","sq_draw"};
        Double pr4 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_square_i"};
        Double pr5 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_square_j"};
        Double pr6 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_triangle_j"};
        Double pr9 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_triangle_draw"};
        Double pr12 = ${PuntajeEspecifico,"Archivo1.js","variable","draw_square_draw"};
        Double pr15 = ${PuntajeEspecifico,"Archivo1.js","variable","x"};
        Double pr16 = ${PuntajeEspecifico,"Archivo1.js","variable","a"};

        Double pe1 = 0;
        Double pe2 = 1;
        Double pe3 = 0;
        Double pe4 = 1;
        Double pe5 = 1;
        Double pe6 = 1;
        Double pe9 = 0;
        Double pe12 = 0;
        Double pe15 = 0;
        Double pe16 = 0;
        String archivo11 = "archivo1.js";
        String titulobarrasesperada = "Probabilidades esperadas para variables archivo Archivo1.js";
        String titulobarrasreal = "Probabilidades obtenidas para variables archivo Archivo1.js";
    }

    #*
    Este comentario debería ser ignorado 189214'!""$%$&"#$"#"#4
    *#

    ##Cargamos los proyectos correspondientes
    COMPARE("C:\Users\angge\OneDrive\Documentos\GitHub\-OLC1-Proyecto1_201901055\CopyAnalyzer\src\Analizadores\Archivo1.js", "C:\Users\angge\OneDrive\Documentos\GitHub\-OLC1-Proyecto1_201901055\CopyAnalyzer\src\Analizadores\Archivo2.js");

    GraficaLineas{
        TiTulO: reporteResumen; 
        ArChIvO: archivo11;
    }

    graficalineas{
        titulo: "Reporte archivo2"; 
        archivo: "archivo2.js";
    }

    ## Agregamos las graficas de barras

    GraficaBarras {
        Titulo: titulobarrasreal;
        Valores: [ pr1, pr2, pr3, pr4, pr5, pr6, pr9, pr12, pr15, pr16 ];
        TituloX: "Nombre de las variables";
        EjeX: [ "triangle_draw", "draw_triangle_i", "sq_draw", "draw_square_i", "draw_square_j", "draw_triangle_j", "draw_triangle_draw", "draw_square_draw", "x", "a" ];
        TituloY: "Puntaje";
    }

    GraficaBarras {
        Titulo: titulobarrasesperada;
        EjeX: [ "triangle_draw", "draw_triangle_i", "sq_draw", "draw_square_i", "draw_square_j", "draw_triangle_j", "draw_triangle_draw", "draw_square_draw", "x", "a" ];
        Valores: [ pe1, pe2, pe3, pe4, pe5, pe6, pe9, pe12, pe15, pe16 ];
        TituloX: "Nombre de las variables";
        TituloY: "Puntaje";
    }

}